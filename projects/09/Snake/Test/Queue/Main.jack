class Main {
    function void main() {
        var Queue q;
        var int x;

        let q = Queue.new();

        // Inserisco alcuni numeri
        do q.push(10);
        do q.push(20);
        do q.push(30);

        // Guardo la coda
        do Output.printString("Tail = ");
        do Output.printInt(q.peekTail());
        do Output.println();   // stampa: 30

        // Guardo la testa (senza toglierla)
        do Output.printString("Head = ");
        do Output.printInt(q.peekHead());
        do Output.println();   // stampa: 10

        // Pop dalla coda (dequeue)
        let x = q.pop();
        do Output.printString("Pop = ");
        do Output.printInt(x);
        do Output.println();   // stampa: 10

        // Ancora pop
        let x = q.pop();
        do Output.printString("Pop = ");
        do Output.printInt(x);
        do Output.println();   // stampa: 20

        // Ultimo pop
        let x = q.pop();
        do Output.printString("Pop = ");
        do Output.printInt(x);
        do Output.println();   // stampa: 30

        // Prova a pop su coda vuota
        let x = q.pop();
        do Output.printString("Pop = ");
        do Output.printInt(x);
        do Output.println();   // stampa: -1 (coda vuota)

        return;
    }
}
